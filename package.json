{
  "name": "@midstallsw/vista",
  "version": "0.1.0",
  "description": "Library for extra components and useful things to make Midstall's websites work better with Nuxt 3",
  "main": "dist/module.cjs",
  "types": "dist/types.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "scripts": {
    "prepare": "husky install",
    "prepack": "npm run build",
    "build": "nuxt-module-build",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "dev:build": "nuxi build playground",
    "dev": "nuxi dev playground",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MidstallSoftware/nuxtlib.git"
  },
  "author": "Tristan Ross",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/MidstallSoftware/nuxtlib/issues"
  },
  "homepage": "https://github.com/MidstallSoftware/nuxtlib#readme",
  "peerDependencies": {
    "@intlify/nuxt3": "^0.2.1",
    "@mdi/font": "^6.6.96",
    "vuetify": "^3.0.0-beta.1"
  },
  "dependencies": {
    "@intlify/core-base": "^9.1.10",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest",
    "date-fns": "^2.28.0",
    "ufo": "^0.8.3",
    "unplugin": "^0.6.2",
    "vue-next-i18n": "^1.0.10"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.4",
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "^9.0.0",
    "commitlint": "^16.2.4",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "nuxt": "^3.0.0-rc.1",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "stylelint": "^14.8.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^3.0.0"
  }
}
